{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/api\\/articles\\/([^/]+?)\\/add-usefull\\.json\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/api\\/articles\\/([^/]+?)\\/add-useless\\.json\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/api\\/articles\\/([^/]+?)\\/likes\\/add\\.json\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/api\\/articles\\/([^/]+?)\\/likes\\/remove\\.json\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/rss\\.xml\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/([^/]+?)\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}